from special_number_finder import Special_Number_Finders
from time import time


#Test Cases
test_cases = [257, 3779, 14741, 331777, 7772777, 111181111, 99999199999]


start_time = time.time()
a = Special_Number_Finders()
for i in test_cases:
    f"The prime number chosen was {i}, the result was:{a._isPrime(i)} ,and the time complexity was {time.time() - start_time}"

